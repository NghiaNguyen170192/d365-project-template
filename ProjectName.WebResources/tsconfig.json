{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "noImplicitAny": false,
    "strictNullChecks": true,
    "removeComments": false,
    "experimentalDecorators": true,
    //"declaration": true,
    "outDir": "out/entities",
    "sourceMap": false,
    "lib": [
      "dom",
      "es6"
    ]
  },
  "include": [
    "src/**/*",
    "typings/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
