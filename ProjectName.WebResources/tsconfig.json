{
  "compileOnSave": true,
  "compilerOptions": {
    "lib": [
      "dom",
      "ES6"
    ],
    "module": "commonjs",
    "noImplicitAny": false,
    "outDir": "out/entities",
    "removeComments": true,
    "sourceMap": false,
    "strictNullChecks": true,
    "target": "ES6",
    "baseUrl": "./",
    "paths": {
      "*": [
        "typings/*",
        "*"
      ]
    },
    "esModuleInterop": true,
    "strict": true,
    "moduleResolution": "Classic",
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmitOnError": true
    
  },
  "include": [
    "src/**/*",
    "typings/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}